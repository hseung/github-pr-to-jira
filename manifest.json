{
  "name": "Open Jira card from Github PR",
  "description" : "For Buzzvill Devs",
  "version": "1.1",
  "browser_action": {
    "defaut_title": "Open new tab of equivilant Jira card",
    "default_icon": "jira.png"
  },
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["autolink.js"]
    }
  ]
}
